{
  "name": "easymage/welcome",
  "description": "Sample welcome magento 2 module using composer",
  "require": {
        "magento/magento-composer-installer": "*"
    },
  "type": "magento2-module",
  "license": "MIT",
   "extra": {
        "map": [
            [
                "*",
                "EasyMage/Welcome"
            ]
        ]
    },
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "EasyMage\\Welcome\\": ""
    },
    "psr-0": {
            "": "app/code/"
        },
  },
  "repositories": [
        {
           "type": "git",
           "url": "https://github.com/rajendra85/easymagewelcome.git"
        }
    ],
}